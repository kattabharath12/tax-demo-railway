
# Database
DATABASE_URL="postgresql://username:password@localhost:5432/tax_filing_db"

# NextAuth
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-secret-key-here"

# JWT
JWT_SECRET="your-jwt-secret-here"

# Server
PORT=8000
NODE_ENV=development

# Google Document AI (Optional)
GOOGLE_CLOUD_PROJECT_ID="your-project-id"
GOOGLE_CLOUD_LOCATION="us"
GOOGLE_CLOUD_W2_PROCESSOR_ID="your-w2-processor-id"
GOOGLE_CLOUD_1099_PROCESSOR_ID="your-1099-processor-id"
GOOGLE_APPLICATION_CREDENTIALS="/path/to/service-account.json"

# AbacusAI
ABACUSAI_API_KEY="your-abacusai-api-key"

# File Upload
MAX_FILE_SIZE=10485760
UPLOAD_DIR=uploads/documents
